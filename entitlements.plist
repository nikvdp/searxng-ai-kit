<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow network access for search functionality -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <!-- Allow outgoing network connections -->
    <key>com.apple.security.network.server</key>
    <false/>
    
    <!-- Disable library validation for broader compatibility -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Allow loading of unsigned libraries if needed -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <false/>
    
    <!-- Runtime hardening -->
    <key>com.apple.security.cs.allow-jit</key>
    <false/>
    
    <!-- File access permissions -->
    <key>com.apple.security.files.user-selected.read-only</key>
    <false/>
    
    <!-- Disable app sandbox for CLI tool -->
    <key>com.apple.security.app-sandbox</key>
    <false/>
</dict>
</plist>